pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('test-deploy') {
            steps {
                sh("kubectl apply -f nginx-deployment.yaml")
            }
        }
        stage('Hello-success') {
            steps {
                echo 'pipeline success'
            } 
        }
    }
}
